{
  "name": "royal-hime",
  "version": "0.0.1",
  "description": "An Experimental HTML5 Game",
  "devDependencies": {
    "lodash": "2.4.1",

    "http-server": "0.6.1",
    "bower": "^1.3.8",

    "mocha": "1.21.4",
    "chai": "1.9.1",

    "istanbul": "0.3.0",

    "browserify": "5.9.3"
  },
  "scripts": {
    "postinstall": "bower install --config.interactive=false",

    "start": "http-server -p 8000 app",

    "pretest": "npm install",
    "test": "export NODE_PATH=src; mocha",
    "test-watch": "export NODE_PATH=src; mocha --watch",

    "coverage": "export NODE_PATH=src; istanbul cover -x \"**/lodash.js\" node_modules/.bin/_mocha",

    "build": "export NODE_PATH=src; browserify src/main.js -o app/scripts/hime.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Prince86EKNJ/royal-hime"
  },
  "author": "Edward Nicholes Jr.",
  "bugs": {
    "url": "https://github.com/Prince86EKNJ/royal-hime/issues"
  },
  "homepage": "https://github.com/Prince86EKNJ/royal-hime"
}
